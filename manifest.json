{
	"manifest_version": 2,
	"name": "棒読みランチャー",
	"description": "棒読みちゃんに自動読み上げさせるChrome拡張機能",
	"version": "1.0.1",
	
	"content_scripts": [
		{
			"matches": [
				"*://www.youtube.com/live_chat*",
				"*://studio.youtube.com/channel/*/livestreaming/dashboard*"
			],

			"js": ["modules/YouTube.js"],

			"all_frames": true,
			"run_at": "document_idle"
		}
	]
}